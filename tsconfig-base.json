{
    "compilerOptions": {
        "alwaysStrict": true,
        "lib": ["es2017", "es6", "dom"],
        "newLine": "lf",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "target": "es5"
    }
}
