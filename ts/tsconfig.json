{
    "extends": "../tsconfig-base.json",
    "compilerOptions": {
        "lib": [
            "es6",
            "es2016",
            "es2016.array.include",
            "es2017.object",
            "dom",
            "dom.iterable"
        ],
        "allowJs": true,
        "module": "amd",
        "moduleResulution": "node",
        "outFile": "../built.js",
        "typeRoots": ["./d"],
        "types": ["requirejs", "fp-ts"],
        "baseUrl": ".",
        "paths": {
            "fp-ts/*": ["../node_modules/fp-ts/*"]
        }
    },
    "include": [
        "./d/*.d.ts",
        "./*.ts"
    ],
    "exclude": []
}
